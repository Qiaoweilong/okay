Shopify.designMode,document.addEventListener("shopify:section:load",function(e){if(console.log("shopify:section:load"),Barracuda.elements.SlideshowObjects["slideshow-"+e.detail.sectionId]){var a=document.querySelector("#slideshow-"+e.detail.sectionId);Barracuda.elements.SlideshowObjects["slideshow-"+e.detail.sectionId].stop(),Barracuda.elements.SlideshowObjects["slideshow-"+e.detail.sectionId]=new BarracudaSlideshow(a)}Barracuda.loadMapsScript(),Barracuda.elements.init(),Barracuda.reInit()}),document.addEventListener("shopify:section:unload",function(e){if(console.log("shopify:section:unload"),Barracuda.elements.SlideshowObjects["slideshow-"+e.detail.sectionId]){var a=document.querySelector("#slideshow-"+e.detail.sectionId);Barracuda.elements.SlideshowObjects["slideshow-"+e.detail.sectionId].stop()}Barracuda.sectionPaddingForBackgroundsTwo(e.target),setTimeout(Barracuda.elements.Overlapping.check.bind(Barracuda.elements.Overlapping),100)}),document.addEventListener("shopify:section:select",function(e){console.log("shopify:section:select"),Barracuda.loadMapsScript(),Barracuda.elements.init(),Barracuda.reInit()}),document.addEventListener("shopify:section:deselect",function(e){console.log("shopify:section:deselect"),Barracuda.elements.SlideshowObjects["slideshow-"+e.detail.sectionId]&&Barracuda.elements.SlideshowObjects["slideshow-"+e.detail.sectionId].start(),Barracuda.loadMapsScript()}),document.addEventListener("shopify:section:reorder",function(e){console.log("shopify:section:reorder"),Barracuda.loadMapsScript(),Barracuda.elements.init(),Barracuda.reInit()}),document.addEventListener("shopify:block:select",function(e){if(console.log("shopify:block:select"),Barracuda.elements.SlideshowObjects["slideshow-"+e.detail.sectionId]){var a=Barracuda.elements.SlideshowObjects["slideshow-"+e.detail.sectionId],s=a.slideshow.querySelector("#blockId-"+e.detail.blockId);a.slideshow.dataset.start=s.dataset.nth,e.detail.load?a.set(parseInt(s.dataset.nth),!1):a.set(parseInt(s.dataset.nth),!0),a.stop()}Barracuda.loadMapsScript()}),document.addEventListener("shopify:block:deselect",function(e){if(console.log("shopify:block:deselect"),Barracuda.elements.SlideshowObjects["slideshow-"+e.detail.sectionId]){var a=Barracuda.elements.SlideshowObjects["slideshow-"+e.detail.sectionId],s=a.slideshow.querySelector("#blockId-"+e.detail.blockId);a.slideshow.dataset.start=s.dataset.nth}Barracuda.sectionPaddingForBackgroundsTwo(),Barracuda.loadMapsScript()});
