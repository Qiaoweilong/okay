let diff=0,current=0,unreached=0,reached=0;const target=2.07,initial=1.98+.01022,divider=2.07-initial;let nextMilestoneText="Loading...";function drawChart(e=100,t=0){var n=google.visualization.arrayToDataTable([["Goal reached","Magic Eden"],["reached volume",t],["missing volume",e]]);new google.visualization.PieChart(document.getElementById("donut_single")).draw(n,{backgroundColor:"#fff9f0",pieHole:.7,pieSliceText:"none",pieSliceTextStyle:{color:"#fff9f0"},pieStartAngle:180,legend:"none",chartArea:{top:0,left:0,width:"100%",height:"100%"},colors:["#30A06D","#bb9772"]})}google.charts.load("current",{packages:["corechart"]}),google.charts.setOnLoadCallback(function(){fetch("https://4lbrculsaf4shwvjizrzeinj6e0lvuha.lambda-url.us-west-1.on.aws").then(async e=>{let t=await e.json();if(unreached=(current=parseFloat(t.total.replace(/[MK]/,"")))>2.07?0:(2.07-current)/divider*100,reached=(diff=current-initial)>1?100:diff/divider*100,nextMilestoneText=`Next milestone 2.07\xe2—Ž`,parseFloat(t.total)>=parseFloat(2.07)){setTimeout(()=>{},300),nextMilestoneText="Milestone reached";let n=document.getElementById("totalVolumeSolana");n.classList.add("animate__animated"),n.classList.add("animate__pulse"),n.classList.add("animate__infinite"),setTimeout(()=>{},3500)}drawChart(unreached,reached),document.getElementById("bearVolume").textContent=t.okb,document.getElementById("totalVolume").textContent=t.total,document.getElementById("coinVolume").textContent=t.okbfc,document.getElementById("boomboxVolume").textContent=t.boombox,document.getElementById("nextMilestone").textContent=nextMilestoneText}),window.addEventListener("resize",e=>{drawChart(unreached,reached)})}),jQuery(window).on("load",function(){"undefined"==typeof wpDataChartsCallbacks&&(wpDataChartsCallbacks={}),wpDataChartsCallbacks[96]=function(e){e.options.tooltip={text:"value"},e.options.pieSliceText="none"}});